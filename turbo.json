{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalDependencies": [".env*", "tsconfig.json", "tsconfig.*.json"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*", "src/**/*", "tsconfig.json", "tsconfig.*.json"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**", "build/**"],
      "cache": true
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "lint": {
      "dependsOn": ["^lint"],
      "inputs": ["$TURBO_DEFAULT$", ".eslintrc*", "eslint.config.*"],
      "outputs": [],
      "cache": true
    },
    "lint:fix": {
      "dependsOn": ["^lint:fix"],
      "inputs": ["$TURBO_DEFAULT$", ".eslintrc*", "eslint.config.*"],
      "outputs": [],
      "cache": false
    },
    "check-types": {
      "dependsOn": ["^check-types"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "tsconfig.json",
        "tsconfig.*.json",
        "src/**/*.ts",
        "src/**/*.tsx"
      ],
      "outputs": [],
      "cache": true
    },
    "typecheck": {
      "dependsOn": ["^typecheck"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "tsconfig.json",
        "tsconfig.*.json",
        "src/**/*.ts",
        "src/**/*.tsx"
      ],
      "outputs": [],
      "cache": true
    },
    "test": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", "src/**/*", "test/**/*", "jest.config.*", "jest.*.json"],
      "outputs": ["coverage/**"],
      "cache": true
    },
    "test:watch": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "test:cov": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", "src/**/*", "test/**/*", "jest.config.*", "jest.*.json"],
      "outputs": ["coverage/**"],
      "cache": true
    },
    "test:debug": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "test:e2e": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", "src/**/*", "test/**/*", "jest-e2e.json"],
      "outputs": ["coverage/**"],
      "cache": true
    },
    "format": {
      "inputs": ["$TURBO_DEFAULT$", ".prettierrc*", "prettier.config.*"],
      "outputs": [],
      "cache": false
    },
    "format:check": {
      "inputs": ["$TURBO_DEFAULT$", ".prettierrc*", "prettier.config.*"],
      "outputs": [],
      "cache": true
    },
    "format:fix": {
      "inputs": ["$TURBO_DEFAULT$", ".prettierrc*", "prettier.config.*"],
      "outputs": [],
      "cache": false
    },
    "start": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "start:debug": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "start:prod": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "generate:component": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
